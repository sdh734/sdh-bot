<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sdh.qqbot.mapper.WinnersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="sdh.qqbot.entity.database.Winners">
        <id column="id" property="id" />
        <result column="prize_id" property="prizeId" />
        <result column="user_id" property="userId" />
        <result column="draw_time" property="drawTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, prize_id, user_id, draw_time
    </sql>

</mapper>
